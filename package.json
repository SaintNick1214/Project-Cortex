{
  "name": "cortex-sdk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "convex dev",
    "predev": "convex dev --until-success && convex dashboard",
    "logs": "convex logs",
    "lint": "tsc -p convex && eslint . --report-unused-disable-directives --max-warnings 0",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathIgnorePatterns=debug --runInBand",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathIgnorePatterns=debug --watch",
    "test:interactive": "tsx tests/interactive-runner.ts",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathIgnorePatterns=debug --coverage",
    "test:debug": "cross-env TEST_DEBUG=true TEST_VERBOSE=true TEST_INSPECT=true node --experimental-vm-modules node_modules/jest/bin/jest.js debug --runInBand"
  },
  "dependencies": {
    "convex": "^1.28.0"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "^1.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
